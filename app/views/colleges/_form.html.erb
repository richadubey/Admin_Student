
  
  <%= form_for(College.new, remote: true) do |f| %>
    
      <div class="form-group" align="center">
        <%= f.label :Name %>
        <%= f.text_field :name %>
      </div>
        
       <div class="form-group" align="center">
        <%= f.label :Address %>
        <%= f.text_field :address %>
       </div>
   

       <div class="form-group" align="center">
        <%= f.label :State %>
        <%= f.text_field :state %>
       </div>

       <div class="form-group " align="center">
        <%= f.label :City %>
        <%= f.text_field :city %>
       </div>

       <div class="form-group" align="center">
        <%= f.label :Phone %>
        <%= f.text_field :phone %>
       
       </div>

      <div class="form-group " align="center">
        <%= f.submit :Submit, class: 'btn btn-success' %>
      </div>
  
  <% end %>

